<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="car" class="com.sd.bean.inheritance.examples.Car">
		<property name="noOfWheels" value="4" />
		<property name="steering" value="power" />
		<property name="music" value="Sony" />
	</bean>

	<bean id="wagnor" class="com.sd.bean.inheritance.examples.WagnoR" parent="car">
		<property name="version" value="VXI" />
	</bean>
</beans>
