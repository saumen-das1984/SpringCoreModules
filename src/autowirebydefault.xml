<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" default-autowire="byType">
	
	<bean id="hostel" class="com.sd.autowire.examples.normal.Hostel">
		<property name="hostelName"
			value="College of Engineering and Management Kolaghat" />
		<property name="city" value="Kolaghat" />
	</bean>

	<bean id="student" class="com.sd.autowire.examples.normal.Student" 
		autowire="default">
		<property name="id" value="CSE0361" />
		<property name="name" value="Saumen" />
	</bean>

</beans>
